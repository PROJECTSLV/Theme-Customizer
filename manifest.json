{
  "manifest_version": 3,
  "name": "GitHub Rainbow Contributions",
  "version": "1.0",
  "description": "Делает график контрибуций радужным",
  "permissions": ["activeTab"],
  "content_scripts": [{
    "matches": ["*://*.github.com/*"],
    "css": ["styles.css"],
    "js": ["content.js"]
  }],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
